<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c22.teamD.controllers.ConfirmExitController">
   <children>
      <VBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: FFCB65; -fx-opacity: .7;">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure you want to exit?" />
            <JFXButton fx:id="exitApplication" buttonType="RAISED" prefHeight="38.0" prefWidth="200.0" style="-fx-background-color: a90000; -fx-text-fill: white;" text="Leave Application" />
            <JFXButton fx:id="cancelApp" buttonType="RAISED" prefHeight="31.0" prefWidth="200.0" style="-fx-background-color: white;" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
